: noop ;

: and { 0 } if jmp
: or { 1 } swap if jmp
: not 0 1 if ;
: toBool 1 0 if ;

: min
    2dup
: .L1
    < {
        drop
    } {
        swap drop
    } if jmp
: max
    2dup swap 'min.L1 jmp

: while
	swap dup call {
		swap dup call
		'while jmp
	} {
		drop drop
	} if jmp

: else
	=then_body ;
: then
var then_body
	then_body {
		then_body swap
	} {
		'noop
	} if call

	0 =then_body

	if jmp


